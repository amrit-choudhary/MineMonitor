html
  head
    script(type='text/javascript', src='https://www.gstatic.com/charts/loader.js')
    script var dataExt = !{dataExt}
    script(type='text/javascript').
      google.charts.load('current', {'packages':['corechart']});
      google.charts.setOnLoadCallback(drawChart);
      function drawChart() {
          var dataExtFinal = [['index', 'service time']];
          var i = 1;
          dataExt.forEach(function(item){
              i++;
              dataExtFinal.push([i, parseInt(item['param1'])]);
          });
        var data = google.visualization.arrayToDataTable(dataExtFinal);
            var options = {
            title: 'Service Time',
            curveType: 'function',
            legend: { position: 'bottom' }
      };
      var chart = new google.visualization.LineChart(document.getElementById('curve_chart'));
      chart.draw(data, options);
      }
  body
    #curve_chart(style='width: 900px; height: 500px')
